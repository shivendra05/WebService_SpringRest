<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<!-- Dependent Bean -->
	<bean id="handler" class="com.beans.RequestHandler"
		scope="prototype" />

	<!--Target bean -->
	<!-- <bean id="container" class="com.beans.WebContainer" scope="singleton"> 
		<constructor-arg ref="handler" /> </bean> -->
		
	<bean id="container" class="com.beans.WebContainer"
		scope="singleton">
		<property name="rHandle" ref="handler" />
	</bean>
	
</beans>
